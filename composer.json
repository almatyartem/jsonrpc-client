{
  "name": "tochka-developers/jsonrpc-client",
  "description": "JsonRpc Client for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Babushkin",
      "email": "darkdarin@tochka.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "illuminate/log": "5.7.*|5.8.*|^6.0",
    "illuminate/cache": "5.7.*|5.8.*|^6.0",
    "illuminate/config": "5.7.*|5.8.*|^6.0",
    "illuminate/container": "5.7.*|5.8.*|^6.0",
    "illuminate/console": "5.7.*|5.8.*|^6.0",
    "illuminate/support": "5.7.*|5.8.*|^6.0",
    "illuminate/pipeline": "5.7.*|5.8.*|^6.0",
    "tochka-developers/jsonrpc-smd": "^1.0",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Tochka\\JsonRpcClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tochka\\JsonRpcClient\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tochka\\JsonRpcClient\\JsonRpcClientServiceProvider"
      ]
    }
  }
}
